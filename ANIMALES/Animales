abstract public class Animales {

//atributos de la clase
 public int edad; 
 public String nombre;
 public int tamaño; 
 
//constructor de la clase 
public Animales { 
 System.out.println("constuctor de un animal");
 }
 
// metodo abstracto de la clase 
public abstract void desplazarse(int forma); // Cuerpo vacío para el método abstracto 

}

